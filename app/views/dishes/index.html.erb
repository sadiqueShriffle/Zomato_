<p style="color: green"><%= notice %></p>

<h1>Dishes</h1>

<div id="dishes">
  <% @dishes.each do |dish| %>
    <%= render dish %>
     
    <p>
      <%= link_to "Show this dish", dish %>
    </p>
  <% end %>
</div>

<% if current_user.type == 'Owner'%>
<%= link_to "New dish", new_category_dish_path%>
<% end%>



